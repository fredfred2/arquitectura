    <service name="CardGameService">
      <implementation-class>games.CardGameService</implementation-class>
    </service>
  </services>
  <clients>
    <client name="CardDeckSessionBeanService">
      <wsdl-url>http://localhost:7001/CardDeckSessionBean/CardDeckSessionBeanService?wsdl</wsdl-url>
      <local-wsdl-file>localhost_7001/CardDeckSessionBean/CardDeckSessionBeanService.wsdl</local-wsdl-file>
      <package-name>ejbs</package-name>
      <catalog-file>catalog.xml</catalog-file>
      <wsimport-options>
